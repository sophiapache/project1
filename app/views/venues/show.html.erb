<h1>
    <%= @venue_info["name"] %>
</h1>

<%= image_tag @venue_info["images"][0]["url"] unless @venue_info["images"].nil? %>

<ul>
    <li><strong>Name:</strong> <%= @venue_info["name"]%></li>
    <li><strong>Address:</strong> <%= @venue_info["address"]["line1"]%><br>
    <%= @venue_info["city"]["name"]%>, <%= @venue_info["state"]["stateCode"]%> <%= @venue_info["country"]["name"]%></li>
</ul>
